[General]
outputPrefix : preprocData_Run2Legacy_2017_CR2_v1
maxEvents : 9999999
outputVariables : *
categories : 7j,8j,9j

[Sample]
name : ttHbb
path : data/datasets/Run2Legacy/2017/ttHTobb_M125_TuneCP5_13TeV-powheg-pythia8.txt
selection : nBDeepFlavL >= 4 and nBDeepFlavM  == 3

[7j]
name : 7J
selection : njets == 7 and ((Wmass4b > 30 and Wmass4b < 60) or (Wmass4b > 100 and Wmass4b < 250))

[8j]
name : 8J
selection : njets == 8 and ((Wmass4b > 30 and Wmass4b < 60) or (Wmass4b > 100 and Wmass4b < 250))

[9j]
name : 9J
selection : njets >= 9 and ((Wmass4b > 30 and Wmass4b < 70) or (Wmass4b > 92 and Wmass4b < 250))
